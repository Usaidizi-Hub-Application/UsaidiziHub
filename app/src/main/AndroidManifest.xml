<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simiyu.usaidizihub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Policy"
            android:parentActivityName=".ClientActivity"></activity>

        <activity android:name=".UploadDataActivity"
            android:parentActivityName=".Policy">

        </activity>

        <activity
            android:name=".ApplicationSettings"
            android:label="@string/title_activity_application_settings"
            android:parentActivityName=".ClientActivity" />

        <activity android:name=".AdminstratorActivity" />

        <activity android:name=".CounselorActivity" />

        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".ClientActivity" />

        <activity android:name=".Registration">
            <!-- Intent filter Registration Activity -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login" />

        <activity android:name=".ClientActivity">

        </activity>
    </application>

</manifest>